{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src/index.js",
      "src/typedef.js",
      "readme.md"
    ]
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": false,
    "sort": true
  }
}
